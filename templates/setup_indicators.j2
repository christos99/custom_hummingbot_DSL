    {#- Initialize indicator configurations -#}
    {%- if indicators %}
    {% for indicator in indicators %}
    {% if indicator.name == "RSI" %}
    # RSI Indicator Setup
    rsi_length = {{ indicator.length }}
    rsi_overbought = {{ indicator.overbought_threshold | default(70) }}
    rsi_oversold = {{ indicator.oversold_threshold | default(30) }}
    {% elif indicator.name == "MACD" %}
    # MACD Indicator Setup
    macd_fast_length = {{ indicator.fast_length }}
    macd_slow_length = {{ indicator.slow_length }}
    macd_signal_smoothing = {{ indicator.signal_smoothing }}
    {% elif indicator.name == "Bollinger Bands" %}
    # Bollinger Bands Setup
    bollinger_length = {{ indicator.length }}
    bollinger_std_dev = {{ indicator.std_dev }}
    {% elif indicator.name == "SMA" %}
    # SMA Indicator Setup
    sma_length = {{ indicator.length }}
    {% elif indicator.name == "EMA"%}
    # EMA Indicator Setup
    ema_length = {{ indicator.length }}
    {% endif %}
    {% endfor -%}
    {%- endif %}